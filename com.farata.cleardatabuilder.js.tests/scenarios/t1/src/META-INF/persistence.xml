<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
	<persistence-unit name="t1">
		<jta-data-source>java:/comp/env/jdbc/orcl</jta-data-source>
		<class>com.engage.entitlement.entity.UserRoleEntity</class>
		<class>com.engage.entitlement.entity.UserEntity</class>
		<class>com.engage.entitlement.entity.RoleEntity</class>
		<class>com.engage.entitlement.entity.CommunityEntity</class>
		<properties>
			<property name="hibernate.dialect" value="org.hibernate.dialect.Oracle10gDialect"/>
		</properties>
	</persistence-unit>
</persistence>
