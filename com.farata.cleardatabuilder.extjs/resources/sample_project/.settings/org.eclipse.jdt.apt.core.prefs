eclipse.preferences.version=1
org.eclipse.jdt.apt.aptEnabled=true
org.eclipse.jdt.apt.genSrcDir=WebContent

org.eclipse.jdt.apt.processorOptions/-Acom.faratasystems.cdb.configuration.file=**{app.name}**/src/hibernate.cfg.xml
org.eclipse.jdt.apt.processorOptions/-Acom.faratasystems.cdb.entities.classpath=**{app.name}**/build/classes

org.eclipse.jdt.apt.processorOptions/-Acom.faratasystems.dto2extjs.output=**{app.name}**/WebContent

org.eclipse.jdt.apt.processorOptions/-Acom.faratasystems.dto2extjs.package-path-transformer=**{app.name}**:app
org.eclipse.jdt.apt.processorOptions/-Acom.faratasystems.dto2extjs.class-name-transformer=**{app.name}**.model

org.eclipse.jdt.apt.processorOptions/-Acom.faratasystems.cdbjs.store.package-path-transformer=**{app.name}**:app
org.eclipse.jdt.apt.processorOptions/-Acom.faratasystems.cdbjs.store.class-name-transformer=**{app.name}**.store
